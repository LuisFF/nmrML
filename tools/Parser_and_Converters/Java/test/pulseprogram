;$Id: Avance2.incl,v 1.10.6.7 2007/01/22 13:54:46 ber Exp $
# 7 "/opt/topspin/exp/stan/nmr/lists/pp/noesygppr1d.t3.be.er" 2

# 1 "/opt/topspin/exp/stan/nmr/lists/pp/Grad.incl" 1
;Grad2.incl  -  include file for Gradient Spectroscopy
;   for 1
;
;avance-version (02/05/31)
;
;$CLASS=HighRes Incl
;$COMMENT=



















define list<gradient> EA=<EA>



;$Id: Grad2.incl,v 1.7.10.3 2007/02/06 13:54:12 ber Exp $
# 8 "/opt/topspin/exp/stan/nmr/lists/pp/noesygppr1d.t3.be.er" 2



"d11=30m"
"d12=20u"
"d15=20u"
"p11=aq"
"d13=3u"

1 ze
  d12 pl12:f3
2 d11 do:f3
3 d12 pl9:f1
  d1 cw:f1 ph29
  4u do:f1
  50u setnmr3|0 setnmr0|34|32|33
  p16:gp1
  d12 pl1:f1
  d15 reset:f1
  d15 reset:f3
  p1 ph1
  4u
  p1 ph2
  d12 pl9:f1
  d8 cw:f1
  4u do:f1
  p16:gp2
  d16 pl1:f1
  4u setnmr3^0 setnmr0^34^32^33
  p0 ph5
  d13 cpds3:f3 ph3:r
  go=2 ph31
  wr #0
  d11 do:f3
exit


ph1=0 2 
ph2=0 0 0 0 0 0 0 0 2 2 2 2 2 2 2 2
ph5=0 0 2 2 1 1 3 3
ph3=0 2 2 0 1 3 3 1 2 0 0 2 3 1 1 3
ph29=0
ph31=0 2 2 0 1 3 3 1 2 0 0 2 3 1 1 3


;pl1 : f1 channel - power level for pulse (default)
;pl9 : f1 channel - power level for presaturation
;p1 : f1 channel -  90 degree high power pulse
;d1 : relaxation delay; 1-5 * T1
;d8 : mixing time
;d11: delay for disk I/O                             [30 msec]
;d12: delay for power switching                      [20 usec]
;NS: 8 * n, total number of scans: NS * TD0
;DS: 4


;use gradient ratio:    gp 1	gp2
;                         50	-10


;for z-only gradients:
;gpz1: 50%

;use gradient files:
;gpnam1: SINE.100



;$Id: $
